<?xml version="1.0" encoding="utf-8"?>
<Page
    x:Class="BasicNavigation.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:BasicNavigation"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid Background="{ThemeResource AcrylicInAppFillColorDefaultBrush}" >
        <TextBlock x:Name="pageTitle" Text="从这里开始"
               Margin="16" Style="{StaticResource TitleTextBlockStyle}"></TextBlock>
        <Border  BorderThickness="1" HorizontalAlignment="Left" CornerRadius="10"  VerticalAlignment="Center" Width=" 120" Height="100">
            <TextBlock Text="    关于此项目" FontWeight="Bold" VerticalAlignment="Top"></TextBlock>

        </Border>
        <HyperlinkButton Content="Github" NavigateUri="https://github.com/nilyhighness/WinUI3-Caculator-Tool" Margin="20"/>
        <HyperlinkButton Content="继续"
                     Click="HyperlinkButton_Click"
                     HorizontalAlignment="Right"
                         VerticalAlignment="Bottom"/>
        
    </Grid>
</Page>
